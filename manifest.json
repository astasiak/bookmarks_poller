{
  "manifest_version": 2,

  "name": "Team bookmarks synchronizer",
  "description": "Synchronizes bookmarks across a team",
  "version": "0.1",

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["lib.js", "background.js"]
  },
  "permissions": [
    "https://*/",
    "http://*/",
    "alarms",
    "bookmarks",
    "storage"
  ]
}
